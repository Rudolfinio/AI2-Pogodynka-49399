{
    "city": "{{ city }}",
    "country": "{{ country }}",
    "weathers": [
        {% for weather in weathers %}
    {
        "date": "{{ weather.date | date('Y-m-d') }}",
        "celsius": "{{ weather.celsius }}",
        "fahrenheit": "{{ weather.fahrenheit }}"
    }{% if not loop.last %},{% endif %}
    {% endfor %}
    ]
}